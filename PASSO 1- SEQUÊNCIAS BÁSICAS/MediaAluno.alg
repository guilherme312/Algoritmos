algoritmo "semnome"
var
   Materia: Caractere
   N1, N2: Inteiro
   M : Real
inicio
  Escreva("Digite Matéria: ")
  Leia(Materia)
  Escreva("Informe nota 1: ")
  Leia(N1)
  Escreva("Informe nota 2: ")
  Leia(N2)
  M <- (N1+N2)/2
  Escreva("Media do aluno em ",Materia," e ingual : ",M)
fimalgoritmo
